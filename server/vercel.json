{
  "version": 2,
  "name": "chat-homeswift-ai",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CORS_ORIGIN": "https://homeswift-ai.vercel.app,https://chat-homeswift-ai.vercel.app"
  }
}
